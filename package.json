{
  "name": "lint-prepush",
  "version": "2.1.0",
  "description": "Run linters on committed files in a Branch🔬",
  "author": "“Theenadayalan” <“puduvai.theena@gmail.com”>",
  "license": "MIT",
  "bin": {
    "lint-prepush": "index.js"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint **/*.js",
    "format": "yarn lint --fix",
    "test": "mocha tests/"
  },
  "lint-prepush": {
    "tasks": {
      "*.js": [
        "node_modules/.bin/eslint"
      ]
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-mocha": "^7.0.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.2.5",
    "mocha": "^8.2.0"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "cosmiconfig": "^6.0.0",
    "debug": "^4.1.1",
    "dedent": "^0.7.0",
    "execa": "^4.0.1",
    "file-system-cache": "^1.0.5",
    "listr": "^0.14.1",
    "log-symbols": "^4.0.0",
    "micromatch": "^4.0.2",
    "npm-which": "^3.0.1"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theenadayalank/lint-prepush.git"
  },
  "bugs": {
    "url": "https://github.com/theenadayalank/lint-prepush/issues"
  },
  "homepage": "https://github.com/theenadayalank/lint-prepush",
  "keywords": [
    "lint",
    "lint-prepush",
    "prepush",
    "husky"
  ],
  "husky": {
    "hooks": {
      "pre-push": "node index.js"
    }
  }
}
