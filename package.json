{
  "name": "lint-prepush",
  "version": "2.2.2",
  "description": "Run linters on committed files in a BranchğŸ”¬",
  "author": "â€œTheenadayalanâ€ <â€œpuduvai.theena@gmail.comâ€>",
  "license": "MIT",
  "bin": {
    "lint-prepush": "./dist/lint-prepush.js"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint **/*.js",
    "lint:fix": "yarn lint --fix",
    "build": "tsup",
    "test": "mocha tests/"
  },
  "lint-prepush": {
    "base": "develop",
    "tasks": {
      "*.js": [
        "node_modules/.bin/eslint"
      ]
    }
  },
  "devDependencies": {
    "chai": "^5.1.0",
    "eslint": "^9.1.1",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^9.0.11",
    "mocha": "^10.4.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cosmiconfig": "^9.0.0",
    "debug": "^4.3.4",
    "dedent": "^1.5.3",
    "execa": "^7.2.0",
    "file-system-cache": "^2.4.4",
    "listr": "^0.14.3",
    "log-symbols": "^6.0.0",
    "micromatch": "^4.0.5",
    "node-which": "^1.0.0",
    "npm-which": "^3.0.1"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/theenadayalank/lint-prepush.git"
  },
  "bugs": {
    "url": "https://github.com/theenadayalank/lint-prepush/issues"
  },
  "homepage": "https://github.com/theenadayalank/lint-prepush",
  "keywords": [
    "lint",
    "lint-prepush",
    "prepush",
    "husky",
    "linter",
    "git"
  ],
  "husky": {
    "hooks": {
      "pre-push": "node ./src/lint-prepush.js"
    }
  }
}
